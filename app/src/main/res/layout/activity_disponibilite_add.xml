<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Medecin.DisponibiliteAdd">
    <LinearLayout
        android:id="@+id/activity_disponibilite_add"
        android:layout_width="368dp"
        android:layout_height="495dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="8dp"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <TextView
            android:id="@+id/activity_disponibilite_jour"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Jour" />


        <EditText
            android:id="@+id/activity_disponibilite_ed_jour"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:ems="10"
            android:inputType="date"
            android:text="" />

        <TextView
            android:id="@+id/activity_disponibilite_heured"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Heure dÃ©but"/>

        <EditText
            android:id="@+id/activity_disponibilite_ed_heured"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:ems="10"
            android:inputType="time"/>

        <TextView
            android:id="@+id/activity_disponibilite_heuref"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Heure Fin"/>

        <EditText
            android:id="@+id/activity_disponibilite_ed_heuref"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:ems="10"
            android:inputType="time"/>

        <Button
            android:id="@+id/activity_disponibilite_ajouter"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Ajouter" />

    </LinearLayout>

</android.support.constraint.ConstraintLayout>