<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Medecin.MedecinAdd">

    <LinearLayout
        android:id="@+id/activity_medecin_add"
        android:layout_width="368dp"
        android:layout_height="495dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="8dp"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">


        <TextView
            android:id="@+id/activity_medecin_email"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Email" />

        <EditText
            android:id="@+id/activity_medecin_ed_email"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:ems="10"
            android:inputType="textEmailAddress"
            android:text="" />

        <TextView
            android:id="@+id/activity_medecin_password"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Mot de passe" />

        <EditText
            android:id="@+id/activity_medecin_ed_password"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:ems="10"
            android:inputType="textPassword" />

        <TextView
            android:id="@+id/activity_medecin_textView2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Nom" />

        <EditText
            android:id="@+id/activity_medecin_ed_nom"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:ems="10"
            android:inputType="textPersonName"
            android:text=""/>

        <TextView
            android:id="@+id/activity_medecin_textView3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Prenom"/>

        <EditText
            android:id="@+id/activity_medecin_ed_prenom"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:ems="10"
            android:inputType="textPersonName"
            android:text="" />

        <TextView
            android:id="@+id/activity_medecin_textView4"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Adresse" />

        <EditText
            android:id="@+id/activity_medecin_ed_adresse"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:ems="10"
            android:inputType="textPostalAddress"
            android:text="" />

        <TextView
            android:id="@+id/activity_medecin_textView5"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Téléphone" />

        <EditText
            android:id="@+id/activity_medecin_ed_telephone"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:ems="10"
            android:inputType="phone"
            android:text="" />

        <TextView android:id="@+id/o11"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"/>
        <TextView
            android:id="@+id/activity_medecin_textView6"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Spécialité"/>

        <Spinner
            android:id="@+id/activity_medecin_specialite"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"/>
        <Button
            android:id="@+id/activity_medecin_button"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Créer" />
    </LinearLayout>

</android.support.constraint.ConstraintLayout>